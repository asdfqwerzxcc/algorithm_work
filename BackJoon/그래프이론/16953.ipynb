    "n,m = map(int,input().split())\n",
    "count=0\n",
    "while n!=m:\n",
    "    if n>m:\n",
    "        count=-2\n",
    "        break\n",
    "    elif str(m)[-1]=='1':\n",
    "        m=m//10\n",
    "        count+=1\n",
    "    elif m%2==0:\n",
    "        m=m//2\n",
    "        count+=1\n",
    "    else:\n",
    "        count=-2\n",
    "        break\n",
    "print(count+1)"
   ]
  }
