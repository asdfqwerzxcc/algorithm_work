    "import sys\n",
    "\n",
    "T=int(input())\n",
    "\n",
    "for i in range(T):\n",
    "    a,b=map(int,sys.stdin.readline().strip().split())\n",
    "    aa,bb=a,b\n",
    "\n",
    "    while bb!=0:\n",
    "        aa=aa%bb\n",
    "        aa,bb=bb,aa\n",
    "\n",
    "    print(a*b//aa)\n"
   ]
  }
